CREATE TABLE nazma_containerItems_tab(
    container_id NUMBER,
    item_id NUMBER,
    status VARCHAR2(20) NOT NULL,
    PRIMARY KEY(container_id, item_id),
    FOREIGN KEY (container_id) REFERENCES nazma_container_tab(container_id) ON DELETE CASCADE,
    FOREIGN KEY (item_id) REFERENCES nazma_foodItem_tab(item_id) ON DELETE CASCADE
);