CREATE TABLE nazma_container_tab(
    container_id NUMBER,
    max_weight NUMBER NOT NULL,
    current_weight NUMBER NOT NULL,
    max_volume NUMBER NOT NULL,
    current_volume NUMBER NOT NULL,
    branch_id NUMBER,
    company_id NUMBER,
    PRIMARY KEY(branch_id, container_id),
    FOREIGN KEY(branch_id, company_id) REFERENCES nazma_branch_tab(branch_id, company_id) ON DELETE CASCADE
);
